        =image_tag "data:image/png;base64,"
      = link_to "https://www.orixlife.co.jp/",class:'logo' do
        =image_tag "data:image/png;base64,"
      = link_to "https://www.lifenet-seimei.co.jp/",class:'logo' do
        =image_tag "data:image/jpeg;base64,"
      = link_to "https://www.fwdfujilife.co.jp/jp",class:'logo' do
